{
  "name": "crna-pull-request-preview",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "exp": "^36.0.0",
    "jest-expo": "^0.2.1",
    "react-native-scripts": "0.0.20",
    "react-test-renderer": "~15.4.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "ci:test": "node node_modules/jest/bin/jest.js --forceExit",
    "ci:deploy-pr": "if [ \"$TRAVIS_PULL_REQUEST\" != \"false\" ]; then yarn ci:deploy; fi",
    "ci:deploy": "yarn ci:exp-login && yarn ci:exp-prepare && yarn ci:exp-publish",
    "ci:exp-login": "exp login -u $EXP_USERNAME -p $EXP_PASSWORD",
    "ci:exp-prepare": "node ./prepare-deploy.js $TRAVIS_PULL_REQUEST_BRANCH",
    "ci:exp-publish": "exp publish"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "14.0.2",
    "react": "~15.4.0",
    "react-native": "0.41.2"
  }
}
